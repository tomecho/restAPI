# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET           /                        controllers.Application.index

# Map static resources from the /public folder to the /assets URL path
GET           /assets/*file            controllers.Assets.versioned(path="/public", file: Asset)


#REST API
#POST          /api/objects             controllers.Application.create
#PUT           /api/objects/:uid        controllers.Application.update(uid: Long)
GET           /api/objects             controllers.Application.showAll
#GET           /api/objects/:uid        controllers.Application.show(uid: Long)
#DELETE        /api/objects/:uid        controllers.Application.delete(uid: Long)
